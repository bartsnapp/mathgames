version 18
__lua__
-- pico8 synth
-- by bart snapp

-- see https://www.lexaloffle.com/bbs/?tid=31079


function _init()
	poke(0x5f2d, 1)
	-- should make the keyboard
	-- accessible
end

function _update()
	while stat(30) do
	if stat(31) == "z" 
		then sfx(0) 
	end 
	if stat(31) == "s"
		then sfx(1)
	end 
	if stat(31) == "x"
		then sfx(2)
	end
	if stat(31) == "d"
		then sfx(3)
	end
	if stat(31) == "c"
	 then sfx(4)
	end
	if stat(31) == "v"
	 then sfx(5)
	end
	if stat(31) == "g"
	 then sfx(6)
	end
	if stat(31) == "b"
		then sfx(7)
	end
	if stat(31) == "h"
	 then sfx(8)
	end
	if stat(31) == "n"
		then sfx(9)
	end
	if stat(31) == "j"
		then sfx(10)
	end
	if stat(31) == "m"
		then sfx(11)
	end
	if stat(31) == "q"
		then sfx(12)
	end
	if stat(31) == "2"
		then sfx(13)
	end
	if stat(31) == "3"
		then sfx(14)
	end
	if stat(31) == "e"
		then sfx(15)
	end
	end
end
